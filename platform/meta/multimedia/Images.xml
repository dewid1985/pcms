<?xml version="1.0"?>
<!DOCTYPE metaconfiguration SYSTEM "meta.dtd">
<metaconfiguration>
    <classes>
        <class name="PlatformMultimediaImages" table="multimedia.images">
            <properties>
                <identifier name="id"/>
                <property name="name" type="String" size="512"/>
                <property name="title" type="String" size="512"/>
                <property name="description" type="String" size="1024"/>
                <property name="tags" type="String" size="512"/>
                <property name="mimeType" type="String" size="128"/>
                <property name="sourceFile" type="String" size="256"/>
                <property name="preparedFile" type="String" size="256"/>
                <property name="icoFile" type="String" size="256"/>
                <property name="uploadedAt" type="TimestampTZ"/>
                <property name="rootDirectory" type="String" size="256"/>

            </properties>
            <pattern name="StraightMapping"/>
        </class>
    </classes>
</metaconfiguration>